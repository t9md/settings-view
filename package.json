{
  "name": "settings-view",
  "version": "0.254.0",
  "main": "./lib/main",
  "description": "Edit config settings, install packages, and change themes",
  "license": "MIT",
  "uriHandler": {
    "method": "handleURI",
    "deferActivation": false
  },
  "dependencies": {
    "async": "~0.2.9",
    "dompurify": "^1.0.2",
    "etch": "0.9.0",
    "fs-plus": "^3.0.0",
    "fuzzaldrin": "^2.1",
    "glob": "4.3.1",
    "hosted-git-info": "^2.1.4",
    "marked": "^0.3.6",
    "request": "^2.83.0",
    "roaster": "^1.1.2",
    "season": "^6.0.2",
    "semver": "^5.3.0",
    "underscore-plus": "^1.0.6"
  },
  "devDependencies": {
    "standard": "^10.0.3",
    "prettier": "^1.10.2",
    "babel-eslint": "^8.2.1",
    "coffeelint": "^1.9.7"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "atom",
      "describe",
      "jasmine",
      "expect",
      "localStorage",
      "URL"
    ],
    "ignore": [
      "spec/fixtures/*"
    ]
  },
  "repository": "https://github.com/atom/settings-view",
  "engines": {
    "atom": "*"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "snippets": {
      "versions": {
        "0.1.0": "consumeSnippets"
      }
    }
  },
  "deserializers": {
    "SettingsView": "createSettingsView"
  }
}
